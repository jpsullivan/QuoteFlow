<div class="aui-group split-view">
    <div class="aui-item list-results-panel">
        <div class="list-panel">
            <div class="list-ordering">
                {{! order by goes in here}}
            </div>
            <div class="list-content">
                {{! list of issues goes in here}}
                {{debug assetPanelData}}

                {{#if assetPanelData}}
                <ol class="issue-list">
                 </ol>
                {{/if}}
            </div>
        </div>
        <div class="end-of-stable-message-container"></div>
        <div class="aui-group pagination-view count-pagination">
            <div class="refresh-container aui-item">
            </div>
            <div class="pagination-container aui-item">
                {{#if pagination}}
                {{/if}}
            </div>
        </div>
    </div>
    <div class="aui-item detail-panel navigator-issue-only">
        <div></div>
    </div>
</div>
